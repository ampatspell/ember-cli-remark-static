    "gray-matter": "^4.0.1",
    "hast-util-to-string": "^1.0.1",
    "prember-crawler": "^0.1.0",
    "rehype-raw": "^3.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-parse": "^5.0.0",
    "remark-rehype": "^3.0.1",
    "unified": "^7.0.0",
    "unist-util-visit": "^1.4.0"
