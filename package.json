{
  "name": "ember-cli-remark-static",
  "version": "0.2.4",
  "description": "Ember.js addon for rendering static markdown by using components.",
  "keywords": [
    "ember-addon",
    "remark",
    "markdown",
    "ast",
    "static"
  ],
  "license": "MIT",
  "author": "Arnis Vuskans <ampatspell@gmail.com> (http://www.amateurinmotion.com/)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ampatspell/ember-cli-remark-static.git"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each",
    "bump": "npm version patch; git push --follow-tags",
    "deploy": "ember build -prod && firebase deploy"
  },
